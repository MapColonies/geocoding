{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "tracing": {
      "isEnabled": false
    },
    "metrics": {
      "enabled": false
    },
    "shared": {},
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": 8080,
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "db": {
    "elastic": {
      "control": {
        "node": "http://localhost:9200",
        "auth": {
          "username": "elastic",
          "password": "changeme"
        },
        "requestTimeout": 60000,
        "index": "control_index"
      },
      "geotext": {
        "node": "http://localhost:9200",
        "auth": {
          "username": "elastic",
          "password": "changeme"
        },
        "requestTimeout": 60000,
        "index": {
          "geotext": "geotext_index",
          "placetypes": "placetypes_index",
          "hierarchies": "hierarchies_index"
        },
        "textTermLanguage": "en"
      }
    },
    "s3": {
      "endpoint": "http://localhost:9000",
      "accessKeyId": "myappuser",
      "secretAccessKey": "myappsecret",
      "forcePathStyle": true,
      "region": "local",
      "bucket": "geocoding",
      "fileName": "table.json"
    },
    "redis": {
      "host": "redis",
      "port": 6379,
      "username": "",
      "password": "",
      "tls": {
        "enabled": false,
        "ca": "",
        "key": "",
        "cert": ""
      },
      "dbIndex": 0,
      "ttl": 600
    }
  },
  "application": {
    "site": "local",
    "services": {
      "tokenTypesUrl": "http://localhost:5000"
    },
    "cronLoadTileLatLonDataPattern": "0 * * * *",
    "elasticQueryBoosts": {
      "name": 1.1,
      "placeType": 1.1,
      "subPlaceType": 1.1,
      "hierarchy": 1.1,
      "viewbox": 1.1,
      "geotextCitiesLayer": 1.1
    },
    "geotextCitiesLayer": "cities_index",
    "roadPlaceTypes": [],
    "sources": {},
    "regions": {},
    "controlObjectDisplayNamePrefixes": {
      "TILE": "Tile",
      "SUB_TILE": "Sub Tile",
      "ROUTE": "Route",
      "ITEM": "Item",
      "CONTROL_POINT": "Control Point",
      "CONTROL_CROSS": "Control Cross",
      "CONTROL_INFRASTRUCTURE": "Control Infrastructure"
    },
    "nameTranslationsKeys": [],
    "mainLanguageRegex": "[a-zA-Z]"
  }
}

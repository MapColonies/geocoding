{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "db": {
    "elastic": {
      "control": {
        "node": "http://localhost:9200",
        "auth": {
          "username": "control",
          "password": "password"
        },
        "requestTimeout": 60000,
        "properties": {
          "index": "control_index",
          "defaultResponseLimit": 3
        }
      },
      "geotext": {
        "node": "http://localhost:9200",
        "auth": {
          "username": "geotext",
          "password": "password"
        },
        "requestTimeout": 60000,
        "properties": {
          "index": {
            "geotext": "geotext_index",
            "placetypes": "placetypes_index",
            "hierarchies": "hierarchies_index"
          },
          "defaultResponseLimit": 3,
          "textTermLanguage": "en"
        }
      }
    },
    "postgresql": {
      "type": "postgres",
      "host": "localhost",
      "port": 5432,
      "username": "postgres",
      "password": "postgres",
      "enableSslAuth": false,
      "sslPaths": {
        "ca": "",
        "key": "",
        "cert": ""
      },
      "database": "postgres",
      "schema": "geocoder"
    },
    "redis": {
      "host": "localhost",
      "port": 6379,
      "username": "",
      "password": "",
      "enableSslAuth": false,
      "sslPaths": {
        "ca": "",
        "key": "",
        "cert": ""
      },
      "database": 0
    }
  },
  "application": {
    "services": {
      "tokenTypesUrl": "http://NLP_ANALYSES"
    },
    "cronLoadTileLatLonDataPattern": "0 * * * *",
    "elasticQueryBoosts": {
      "name": 1.1,
      "placeType": 1.1,
      "subPlaceType": 1.1,
      "hierarchy": 1.1,
      "viewbox": 1.1
    },
    "sources": {
      "SOURCE_A": "a",
      "SOURCE_B": "b"
    },
    "regions": {},
    "nameTranslationsKeys": ["en", "fr"],
    "mainLanguageRegex": "[a-zA-Z]"
  }
}

{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "db": {
    "elastic": {
      "control": {
        "node": "http://control_elastic:9200",
        "auth": {
          "username": "elastic",
          "password": "changeme"
        },
        "requestTimeout": 60000,
        "properties": {
          "index": "control_index"
        }
      },
      "geotext": {
        "node": "http://geotext_elastic:9200",
        "auth": {
          "username": "elastic",
          "password": "changeme"
        },
        "requestTimeout": 60000,
        "properties": {
          "index": {
            "geotext": "geotext_index",
            "placetypes": "placetypes_index",
            "hierarchies": "hierarchies_index"
          },
          "textTermLanguage": "en"
        }
      }
    },
    "s3": {
      "endpoint": "http://s3:9000",
      "credentials": {
        "accessKeyId": "accessKeyId",
        "secretAccessKey": "secretAccessKey"
      },
      "forcePathStyle": true,
      "region": "local",
      "files": {
        "latLonConvertionTable": {
          "bucket": "geocoding",
          "fileName": "table.json"
        }
      }
    },
    "redis": {
      "host": "localhost",
      "port": 6379,
      "username": "",
      "password": "",
      "enableSslAuth": false,
      "sslPaths": {
        "ca": "",
        "key": "",
        "cert": ""
      },
      "database": 0
    }
  },
  "application": {
    "services": {
      "tokenTypesUrl": "http://NLP_ANALYSES"
    },
    "cronLoadTileLatLonDataPattern": "0 * * * *",
    "elasticQueryBoosts": {
      "name": 1.1,
      "placeType": 1.1,
      "subPlaceType": 1.1,
      "hierarchy": 1.1,
      "viewbox": 1.1
    },
    "sources": {},
    "regions": {},
    "controlObjectDisplayNamePrefixes": {},
    "nameTranslationsKeys": [],
    "mainLanguageRegex": "[a-zA-Z]"
  }
}

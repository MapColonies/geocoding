{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "db": {
    "elastic": {
      "node": "http://localhost:9200",
      "auth": {
        "username": "elastic",
        "password": "changeme"
      },
      "requestTimeout": 60000,
      "properties": {
        "index": "myIndex",
        "size": 3
      }
    }
  }
}
